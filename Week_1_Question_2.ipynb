        "#Question 2\n",
        " \n",
        "import matplotlib.pyplot as plt\n",
        " \n",
        "x_values = {-3,-2,-1,1,2,3,4,5,6,7}\n",
        "for x in x_values:\n",
        "  #calculate the value of the quadratic function\n",
        "  y = x**2 + 2*x + 1\n",
        "  plt.plot(x,y,'.')\n",
        "  print('x={0} y={1}'.format(x, y))\n"
      ]
